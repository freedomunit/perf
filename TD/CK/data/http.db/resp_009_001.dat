    (function($){ 
    	$.fn.excel2htmlcopy = function(){//Written by Daniel Ruiz
    		var element = this;
    		$(element).on('paste', ':text:enabled', function(e){
    		    e.preventDefault();
    		    var cellIndex = $(this).parents('td').index(),
    		        rowIndex = $(this).parents('tr').index(),
    		        trth = $(element).find('thead tr').length-1,
    		        rowStartIndex = rowIndex+trth,
    		        str = (window.clipboardData ? window.clipboardData.getData('Text')/*For IE*/ : (e.originalEvent || e).clipboardData.getData('text/plain')/*For Webkit/Firefox*/);

    		        var tempArray = str.replace(/\r?\n|\r/g, '|').split('|'); // Split at Carriage Return, Lines Feeds etc. this covers Win, Unix/New Macs, Old Macs
    		        var tempArraylength = tempArray.length-1;
    		        var finalArray = [];
    		        
    		        if (tempArray[tempArraylength]==""){ // Need this to trim blank array that Win adds
    		        	tempArray.pop();
    		        	}
    		       
    		        for (var i = 0; i < tempArray.length; i++) {  
    		            finalArray[i] = tempArray[i].split(/\t/g); //Split at tab. Excel returns column content with tabs separating them
    		        }
    		          var rowMax = tempArraylength+rowIndex;
    		          $(element).find('tr:gt('+rowStartIndex+'):lt('+rowMax+')').each(function(i) { 
    		        	  colIdx = 0;
    		                $(this).find('td:eq('+cellIndex+') :text, td:gt('+(cellIndex)+'):visible :text').each(function(j) {  
    		                	 if (finalArray[i][colIdx] != undefined){
    		                		 $(this).val($.trim(finalArray[i][colIdx]));
    		                		 colIdx++;
    		                	 }
    		                    });
    		          });
    		  });
    	};
    })(jQuery); 
    
