//************* Clear text input boxes in the given table by text id
function clearTextboxInTableById(tableId,cellId){
   var objTable = document.getElementById(tableId); 	
   var ckBx=objTable.getElementsByTagName('input');			
   for (i=0; i<ckBx.length;i++) {
   // -- clear those input boxes with the given cellId
	   if (ckBx[i].getAttribute('type')=='text' && ckBx[i].id == cellId) {					
	   		ckBx[i].value = "";
	   }
   }
}
