function swipe(event, direction){
	if (direction == "left"){
			history.forward();
		}else if (direction == "right"){
			history.back();
		}
}

//START #editclusters, #clustertotals2 CLUSTER DOOR TOTALS
function clustertotals(){
	var g = $('input[type=text]', '#editclusters ').filter(function() { return this.value.length !== 0 }).length,
		h = $('input[type=text]', '#clustertotals2').filter(function() { return this.value.length !== 0 }).length;
	
	if (g>0){
		$('input[type=text]','#clustertotals2').prop('disabled','disabled').css('background-color','#d4d0c8');
	}else if (h>0){
		$('input[type=text]','#editclusters').prop('disabled','disabled').css('background-color','#d4d0c8');
	}
	
	$('body').on({paste: function(){	
			p='1';
			var something = this;
			setTimeout(function(){
				$('#editclusters tbody tr td').each(function(){
				    var eq = $(this).index();
				    b = parseInt($('td:eq(' + eq + ') input[type=text]', '#editclusters tbody tr').filter(function() { return this.value.length !== 0 }).length);	  	
				    $('#editclusters').find('tr:last td.ctotal:eq(' + eq + ')').css('font-weight','bold').text(b); 
				    $('input[type=text]','#clustertotals2').prop('disabled','disabled').css('background-color','#d4d0c8');
				    var Gsum = 0;
				   $('.ctotal', '#editclusters').each(function() {
					      var val = parseInt($(this).text());
				         if(isNaN(val)||val===undefined){return} 
				           Gsum += val; 
				        }); 
				  	$('.gtotal','#editclusters').css('font-weight','bold').text(Gsum);  
				});
			},100);
		},
		keyup: function(event){
			if (p!='1'){
				clustersedit(this);
			}
		},
		keydown: function(e){
			var charCode =(e.which) ? e.which : e.keyCode;
			if (charCode == 46){ //Delete needs to be here browser issue
				$(this).select();
            }else if (!(charCode == 8 || charCode == 9 || charCode == 16 || charCode == 32 || charCode >= 37  && charCode <= 40 || charCode == 45 || charCode >= 48  && charCode <= 57 || charCode >= 65  && charCode <= 90 || charCode >= 96 && charCode <= 105 || charCode == 109 || charCode == 173 || charCode == 189 )) { e.preventDefault();                                                                                                                                              
            }else if(e.which>=65 && e.which<=90 && $(this).closest('table').attr('id')=='clustertotals2'){ e.preventDefault();
            }else if(e.which==39){ $(this).closest('td').next().find('input').focus();
            }else if(e.which==37){ $(this).closest('td').prev().find('input').focus();
            }else if(e.which==40){ $(this).closest('tr').next().find('td:eq('+$(this).closest('td').index()+')').find('input').focus();
            }else if(e.which==38){ $(this).closest('tr').prev().find('td:eq('+$(this).closest('td').index()+')').find('input').focus();}
		},
		focus: function(){ 
			p='0';
			$(this).select();
		},
		mouseup: function(e){ e.preventDefault(); }
		}, '#editclusters input[type=text]:enabled, #clustertotals2 input[type=text]:enabled');  
	
	
    $('body').on('click', '.clearall', function(e){
    	e.preventDefault();
    	var id = $(this).parents('table').prop('id');
    	$( 'input[type=text]','#'+id).each(function() {
    		        $(this).val('');
    		    });
    	clustersedit(this);
    });
    
    $('body').on('click', '.clearc', function(e){
    	e.preventDefault();
    	var i = $(this).parent('td').index();
    	var id = $(this).parents('table').prop('id');
    	$('td:eq(' + i + ') input[type=text]', '#'+id+' tbody tr').each(function() {
    		        $(this).val('');
    		    });
    	clustersedit(this);
    });
};
function clustersedit(t){
	var g = $('input[type=text]', '#editclusters').filter(function() { return this.value.length !== 0 }).length,
		h = $('input[type=text]', '#clustertotals2').filter(function() { return this.value.length !== 0 }).length;

		if (g==h){ 
				$('input[type=text]', '#editclusters, #clustertotals2').prop('disabled','').css('background-color','');
				$('td:last','#clustertotals2').text('');
				$('tr:not(.clearrow)', '#editclusters').find('td:last, .ctotal').text('');
				return;
		}else if (h > 0) { 	
		//START #clustertotals2 
				$('input[type=text]','#editclusters').prop('disabled','disabled').css('background-color','#d4d0c8');
				Rsum = 0;
				$('input[type=text]', '#clustertotals2').each(function() {
					$(this).prop('disabled','');
		               var val = parseInt($(this).val(),10);
		               if(isNaN(val)||val===undefined){return} 
		               Rsum += val; 
		            }); 
		    	$('td:last','#clustertotals2').css('font-weight','bold').text(Rsum);  		
		//END  #clustertotals2 
		}else if (g > 0){ 
		//START #editclusters
			$('input[type=text]','#clustertotals2').prop('disabled','disabled').css('background-color','#d4d0c8');
			$('input[type=text]','#editclusters').prop('disabled','').css('background-color','');
				var Gsum = 0,
				eq = $(t).parents('td').index(),
				b = parseInt($('td:eq(' + eq + ') input[type=text]', '#editclusters tbody tr').filter(function() { return this.value.length !== 0 }).length);
		  	$('#editclusters').find('tr:last td:eq(' + eq + ')').css('font-weight','bold').text(b);
		  	$('.ctotal', '#editclusters').each(function() {
		           var val = parseInt($(this).text());
		           if(isNaN(val)||val===undefined){return} 
		           Gsum += val; 
		        }); 
		  	$('.gtotal','#editclusters').css('font-weight','bold').text(Gsum);  
		//END #editclusters	
		};
    };  
 //END #editclusters, #clustertotals2 CLUSTER DOOR TOTALS

