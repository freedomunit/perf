			  $("#parent-text").css('display','none');
			  $("#parent-select select").css('display','inline-block');
		});
	});
  
	function corporateaccess(){
        var str = "";
        $("#corporate-select select option:selected").each(function () {
            str += $(this).text() + " ";
        });
        $("#corporate-text").text(str);
	};
	
	function parentaccess(){
		if($("#parent-select select option").length > 1){
			var str = "";
			$("#parent-select select option:selected").each(function () {
			    str += $(this).text() + " ";
			});
			$("#parent-text").text(str);
		}else{
			$("#parent-text").css('display','none');
            $("#parent-select").css('display','none');
		}
	};
</script>

